         DATASET DD1=FOLDIND,DEVICE=3380
         SEGM   NAME=FOLDINS,PARENT=0,BYTES=36
         FIELD  NAME=(FOLDINF,SEQ,U),BYTES=36,START=1,TYPE=C
         LCHILD NAME=(FOLDER,FOLDDBD),INDEX=FNAME
         DBDGEN
         FINISH
         END
